{
  "name": "rollup-plugin-svgr",
  "version": "1.0.0",
  "description": "simple rollup plugin svgr without svgo or babel",
  "keywords": [],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (https://christophe.hurpeau.com)",
  "license": "ISC",
  "repository": "https://github.com/christophehurpeau/rollup-plugin-svgr.git",
  "homepage": "https://github.com/christophehurpeau/rollup-plugin-svgr",
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "./lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "clean:build": "true",
    "lint": "yarn run lint:prettier && yarn run lint:eslint",
    "lint:eslint": "eslint --report-unused-disable-directives --resolve-plugins-relative-to . --quiet *.{js,cjs,mjs} lib",
    "lint:prettier": "pob-root-prettier --check .",
    "lint:prettier:fix": "pob-root-prettier --write .",
    "postinstallDev": "pob-root-postinstall",
    "test": "jest",
    "test:coverage": "jest  --coverage --coverageReporters=json --coverageReporters=text",
    "test:watch": "jest --watch"
  },
  "prettier": "@pob/root/prettier-config",
  "commitlint": {
    "extends": [
      "@pob/commitlint-config"
    ]
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache/jest",
    "testMatch": [
      "<rootDir>/lib/**/__tests__/**/*.?(m)js",
      "<rootDir>/lib/**/*.test.?(m)js"
    ],
    "collectCoverageFrom": [
      "lib/**/*.?(m)js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "testEnvironment": "node"
  },
  "pob": {},
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "peerDependenciesMeta": {
    "@babel/core": {
      "optional": true
    }
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.2.0",
    "@svgr/core": "^8.0.0",
    "@svgr/plugin-jsx": "^6.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.22.5",
    "@babel/preset-env": "7.22.5",
    "@babel/preset-react": "7.22.5",
    "@pob/commitlint-config": "6.0.0",
    "@pob/eslint-config": "50.0.5",
    "@pob/root": "8.0.0",
    "@types/jest": "29.5.2",
    "eslint": "8.41.0",
    "jest": "29.5.0",
    "rollup": "2.79.1"
  }
}
